@model Langben.DAL.APPLIANCECOLLECTION

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.GETNUMBER) :
                </div>
                <div class="display-field" id='GETNUMBER'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATETIME) :
                </div>
                <div class="display-field" id='CREATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATEPERSON) :
                </div>
                <div class="display-field" id='CREATEPERSON'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATETIME) :
                </div>
                <div class="display-field" id='UPDATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATEPERSON) :
                </div>
                <div class="display-field" id='UPDATEPERSON'>
                </div>        
                <div class="display-label">
                      @Html.LabelFor(model => model.APPLIANCE_DETAIL_INFORMATIONID) :
                </div>
                <div class="display-field">
                    @if (Model.APPLIANCE_DETAIL_INFORMATION != null)
                       {
                    @Model.APPLIANCE_DETAIL_INFORMATION.BAR_CODE_NUM
                    } 
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/APPLIANCECOLLECTIONApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#GETNUMBER").html(res.GETNUMBER);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);
            }
        })
    });
</script>

