        }
    }
    //B类增加行    
    var addRow = function (obj) {        
        var trNew = $('#tongdao_B tbody').find('tr:last').clone();          
        $(trNew).children().eq(5).find('input').eq(0).val("");
        $(trNew).children().find('input').each(function () {
