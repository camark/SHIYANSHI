        function GetValue() {
            var hideDangQianTongDao = GetTongDao();//当前操作第几通道
            var datas = {
                inputMinValue: $.trim($(".clone #shurufanwei_" + hideDangQianTongDao + "_1_1").val()) == "" ? "" : parseFloat($.trim($(".clone #shurufanwei_" + hideDangQianTongDao + "_1_1").val())),
