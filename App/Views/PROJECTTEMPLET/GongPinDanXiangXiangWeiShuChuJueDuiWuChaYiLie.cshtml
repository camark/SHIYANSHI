@section JS {
    <script type="text/javascript" language="javascript">
        //一个量程的需要这个方法
        //重写set方法，只准对增加空白行
        function set(tbodyIndex) {

            $('#dlg').dialog('open');//打开弹出框

            $("#hideDangQianTongDao").val(tbodyIndex);

        }
        //增加行
        function CreateRow() {

            var hideDangQianTongDao = $("#hideDangQianTongDao").val();//当前操作第几通道
            var LiangChengShuLiang = $("#tongdao_" + hideDangQianTongDao).find("#hideLiangChengShuLiang").val();//某个通道量程数量
            LiangChengShuLiang++;
            var rowspanIdx = $("#txtPoint").val();     //检测点数
            txtPointLen = $("#txtPointLen").val(); //小数点位数
            var standardValue = "";
            var htmlString = [];
            var rowLength = $("#tbody_" + hideDangQianTongDao + " tr").length;//行号

            var lianChengID = hideDangQianTongDao + "_" + LiangChengShuLiang;//例如：第几个通道_第几个量程

            var temp = 30;
            for (var rowidx = 0; rowidx < rowspanIdx; rowidx++) {
                rowLength++;
                htmlString.push("<tr rowindex='" + rowLength + "'>");

                var rowspan = 1; //合并单元格行数

                var txtVal = "";//(文本框值)，如果有值并且行号为null直接赋值，否则走自动计算
                var unit = "<sup>°</sup>";//在输入框后面的单位


                var HangHaoID = lianChengID + "_" + rowLength;//量程ID_第几行

                //计算默认值
                if (rowspanIdx == 6)
                { temp = 60 }
                else if (rowspanIdx == 12)
                { temp = 30 }
                else
                    temp = "";
                txtVal = (rowidx * temp).toFixed(2);
                @for (int i = 0; i < 3; i++)//一共3列
                {
                    for (int c = 0; c < ViewBag.canshu; c++)//组
                    {
                        if (i == 0)
                        {
                            ViewBag.canshu2 += @"htmlString.push(SetTDHtml(rowspan, 'XIANSHIZHIER"
 + c + "', HangHaoID, rowidx, "
+ ((ViewBag.ceLiangShuChu == "相位输出") ? "txtVal" : "''") + ", '', unit,'jueDuiwuCha1'));";

                        }
                        else if (i == 1)
                        {
                            ViewBag.canshu2 += @"htmlString.push(SetTDHtml(rowspan, 'BIAOZHUNZHIER"
             + c + "', HangHaoID, rowidx, "
            + ((ViewBag.ceLiangShuChu == "相位输出") ? "''" : "txtVal") + ", '', unit,'jueDuiwuCha2'));";

                        }
                        else if (i == 2)
                        {
                            ViewBag.canshu2 += @"htmlString.push(SetTDHtml(rowspan, 'YINYONGWUCHAER"
             + c + "', HangHaoID, rowidx, '', '', unit));";

                        }
                    }


                }


                if (rowidx == 0) {
                    //需要合并的列
                }

                //相位
                htmlString.push(SetTDHtml(rowspan, "READVALUE", HangHaoID, rowidx, txtVal, "", unit));

                //动态计算出的默认值列
                @Html.Raw(ViewBag.canshu2)

                //校准结果的不确定度 U(K =
                htmlString.push(SetTDHtml(rowspan, "UNCERTAINTYDEGREE", HangHaoID, rowidx, '', ""));

                htmlString.push("</tr>");
            }
            htmlString = htmlString.join("");

            var tagRow = $("#tongdao_" + hideDangQianTongDao).find("#tbody_" + hideDangQianTongDao).append(htmlString);
            $.parser.parse(tagRow);//渲染easyui组建
            $('#dlg').window('close');//关闭弹出框
            $("#tongdao_" + hideDangQianTongDao).find("#tbody_" + hideDangQianTongDao).find("#hideLiangChengShuLiang").val(LiangChengShuLiang);
        }

        //离开事件，用于计算
        function blurValue(obj) {

            //   LianDongShiJiZhi(obj);
        }
        //离开事件，用于计算
        function jueDuiwuCha1(obj) {
            //重新计算当前行
            var name = $(obj).attr("name");
            var id = $(obj).attr("id");
            var idAdd = id.substring(id.indexOf('_')-1);
            
            var biaoZhunData = $(obj).parent().parent().find("#BIAOZHUNZHIER"  + idAdd).val();
            var showData = $(obj).val();
            if (showData != "" && biaoZhunData != "") {
                var txtPointLen = $("#txtPointLen").val(); //小数点位数
                var xiangDuiWuCha = (showData - biaoZhunData);
                var data = xiangDuiWuCha.toFixed(txtPointLen);
                $(obj).parent().parent().find("#YINYONGWUCHAER"  + idAdd).val(data);
            }
        }
        function jueDuiwuCha2(obj) {
            //重新计算当前行
            var name = $(obj).attr("name");
            var id = $(obj).attr("id");
            var idAdd = id.substring(id.indexOf('_') - 1);

            var showData = $(obj).parent().parent().find("#XIANSHIZHIER" + idAdd).val();
            var biaoZhunData = $(obj).val();
            if (showData != "" && biaoZhunData != "") {
                var txtPointLen = $("#txtPointLen").val(); //小数点位数
                var xiangDuiWuCha = (showData - biaoZhunData);
                var data = xiangDuiWuCha.toFixed(txtPointLen);
                $(obj).parent().parent().find("#YINYONGWUCHAER" + idAdd).val(data);
            }
        }




    </script>


}
