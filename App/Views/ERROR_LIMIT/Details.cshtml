@model Langben.DAL.ERROR_LIMIT

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.ACCURACY_GRADE) :
                </div>
                <div class="display-field" id='ACCURACY_GRADE'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.LIMIT_VALUE) :
                </div>
                <div class="display-field" id='LIMIT_VALUE'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CATEGORY) :
                </div>
                <div class="display-field" id='CATEGORY'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATETIME) :
                </div>
                <div class="display-field" id='CREATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATEPERSON) :
                </div>
                <div class="display-field" id='CREATEPERSON'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATETIME) :
                </div>
                <div class="display-field" id='UPDATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATEPERSON) :
                </div>
                <div class="display-field" id='UPDATEPERSON'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/ERROR_LIMITApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#ACCURACY_GRADE").html(res.ACCURACY_GRADE);$("#LIMIT_VALUE").html(res.LIMIT_VALUE);$("#CATEGORY").html(res.CATEGORY);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);
            }
        })
    });
</script>

