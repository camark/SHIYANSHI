@model Langben.DAL.RULE

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.NAMEOTHER) :
                </div>
                <div class="display-field" id='NAMEOTHER'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.NAME) :
                </div>
                <div class="display-field" id='NAME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.SCHEME_MENU) :
                </div>
                <div class="display-field" id='SCHEME_MENU'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.SORT) :
                </div>
                <div class="display-field" id='SORT'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.IS_UNCERTAINTY) :
                </div>
                <div class="display-field" id='IS_UNCERTAINTY'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UNCERTAINTY_MENU) :
                </div>
                <div class="display-field" id='UNCERTAINTY_MENU'>
                </div>        
                <div class="display-label">
                      @Html.LabelFor(model => model.UNDERTAKE_LABORATORYID) :
                </div>
                <div class="display-field">
                    @if (Model.UNDERTAKE_LABORATORY != null)
                       {
                    @Model.UNDERTAKE_LABORATORY.NAME
                    } 
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.INPUTSTATE) :
                </div>
                <div class="display-field" id='INPUTSTATE'>
                </div>        
                <div class="display-label">
                      @Html.LabelFor(model => model.PARENTID) :
                </div>
                <div class="display-field">
                    @if (Model.RULE2 != null)
                       {
                    @Model.RULE2.NAMEOTHER
                    } 
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATETIME) :
                </div>
                <div class="display-field" id='CREATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATEPERSON) :
                </div>
                <div class="display-field" id='CREATEPERSON'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATETIME) :
                </div>
                <div class="display-field" id='UPDATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATEPERSON) :
                </div>
                <div class="display-field" id='UPDATEPERSON'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/RULEApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#NAMEOTHER").html(res.NAMEOTHER);$("#NAME").html(res.NAME);$("#SCHEME_MENU").html(res.SCHEME_MENU);$("#SORT").html(res.SORT);$("#IS_UNCERTAINTY").html(res.IS_UNCERTAINTY);$("#UNCERTAINTY_MENU").html(res.UNCERTAINTY_MENU);$("#INPUTSTATE").html(res.INPUTSTATE);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);
            }
        })
    });
</script>

