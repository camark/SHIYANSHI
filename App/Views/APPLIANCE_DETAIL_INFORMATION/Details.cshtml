<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/APPLIANCE_DETAIL_INFORMATIONApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#BAR_CODE_NUM").html(res.BAR_CODE_NUM);$("#APPLIANCE_NAME").html(res.APPLIANCE_NAME);$("#MODEL").html(res.MODEL);$("#FORMAT").html(res.FORMAT);$("#FACTORY_NUM").html(res.FACTORY_NUM);$("#NUM").html(res.NUM);$("#ATTACHMENT").html(res.ATTACHMENT);$("#APPEARANCE_STATUS").html(res.APPEARANCE_STATUS);$("#MAKE_ORGANIZATION").html(res.MAKE_ORGANIZATION);$("#REMARKS").html(res.REMARKS);$("#END_PLAN_DATE").html(res.END_PLAN_DATE);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);$("#APPLIANCE_RECIVE").html(res.APPLIANCE_RECIVE);$("#APPLIANCE_PROGRESS").html(res.APPLIANCE_PROGRESS);$("#ORDER_STATUS").html(res.ORDER_STATUS);$("#ISOVERDUE").html(res.ISOVERDUE);$("#OVERDUE").html(res.OVERDUE);$("#STORAGEINSTRUCTIONS").html(res.STORAGEINSTRUCTIONS);$("#STORAGEINSTRUCTI_STATU").html(res.STORAGEINSTRUCTI_STATU);
            }
        })
    });
</script>

