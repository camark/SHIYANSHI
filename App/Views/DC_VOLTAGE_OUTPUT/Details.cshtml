<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/DC_VOLTAGE_OUTPUTApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#RANGE").html(res.RANGE);$("#RANGE_UNIT").html(res.RANGE_UNIT);$("#OUTPUT_VALUE").html(res.OUTPUT_VALUE);$("#OUTPUT_VALUE_UNIT").html(res.OUTPUT_VALUE_UNIT);$("#ACTUAL_OUTPUT_VALUE").html(res.ACTUAL_OUTPUT_VALUE);$("#ACTUAL_OUTPUT_VALUE_UNIT").html(res.ACTUAL_OUTPUT_VALUE_UNIT);$("#RELATIVE_ERROR").html(res.RELATIVE_ERROR);$("#RELATIVE_ERROR_UNIT").html(res.RELATIVE_ERROR_UNIT);$("#UNCERTAINTY_DEGREE").html(res.UNCERTAINTY_DEGREE);$("#UNCERTAINTY_DEGREE_UNIT").html(res.UNCERTAINTY_DEGREE_UNIT);$("#INDEX1").html(res.INDEX1);$("#INDEX2").html(res.INDEX2);$("#RESOLUTION").html(res.RESOLUTION);$("#REFERENCE_RANGE").html(res.REFERENCE_RANGE);$("#PREPARE_SCHEMEID").html(res.PREPARE_SCHEMEID);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);
            }
        })
    });
</script>

