@model Langben.DAL.TEST_ITEM_FORMAT

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.ROW_COUNT) :
                </div>
                <div class="display-field" id='ROW_COUNT'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.COLUMN_COUNT) :
                </div>
                <div class="display-field" id='COLUMN_COUNT'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CHANNEL_NUMBER) :
                </div>
                <div class="display-field" id='CHANNEL_NUMBER'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATETIME) :
                </div>
                <div class="display-field" id='CREATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATEPERSON) :
                </div>
                <div class="display-field" id='CREATEPERSON'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATETIME) :
                </div>
                <div class="display-field" id='UPDATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATEPERSON) :
                </div>
                <div class="display-field" id='UPDATEPERSON'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/TEST_ITEM_FORMATApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#ROW_COUNT").html(res.ROW_COUNT);$("#COLUMN_COUNT").html(res.COLUMN_COUNT);$("#CHANNEL_NUMBER").html(res.CHANNEL_NUMBER);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);
            }
        })
    });
</script>

