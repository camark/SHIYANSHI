@model Langben.DAL.THREE_PHASE_UNCERTAINTY

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.DIGIT) :
                </div>
                <div class="display-field" id='DIGIT'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.DEGREE1) :
                </div>
                <div class="display-field" id='DEGREE1'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.DEGREE2) :
                </div>
                <div class="display-field" id='DEGREE2'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.DEGREE3) :
                </div>
                <div class="display-field" id='DEGREE3'>
                </div>        
                <div class="display-label">
                      @Html.LabelFor(model => model.RULEID) :
                </div>
                <div class="display-field">
                    @if (Model.RULE != null)
                       {
                    @Model.RULE.NAME
                    } 
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATETIME) :
                </div>
                <div class="display-field" id='CREATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATEPERSON) :
                </div>
                <div class="display-field" id='CREATEPERSON'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATETIME) :
                </div>
                <div class="display-field" id='UPDATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATEPERSON) :
                </div>
                <div class="display-field" id='UPDATEPERSON'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/THREE_PHASE_UNCERTAINTYApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#DIGIT").html(res.DIGIT);$("#DEGREE1").html(res.DEGREE1);$("#DEGREE2").html(res.DEGREE2);$("#DEGREE3").html(res.DEGREE3);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);
            }
        })
    });
</script>

