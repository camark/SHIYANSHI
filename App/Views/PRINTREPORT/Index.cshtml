
@model Langben.DAL.PRINTREPORT 
@{ Layout = "~/Views/Shared/Index.cshtml"; }
@using Common 
@using Models
<script src="~/Scripts/JScriptIndex.js"></script>
 
<script type="text/javascript" language="javascript">
        $(function () {

            $('#flexigridData').datagrid({
                title: '打印报告记录', //列表的标题
                iconCls: 'icon-site',
                width: 'auto',
                height: 'auto',
                //nowrap: false,
                striped: true,
                collapsible: true,
                url: '/api/PRINTREPORTApi/PostData/@ViewBag.ID', //获取数据的url
                sortName: 'ID',
                sortOrder: 'desc',
                idField: 'ID',
                 
                columns: [[
                   
                    
					 { field: 'CREATETIME', title:  '打印时间', width: 155
                    , formatter: formatDatebox
}
					,{ field: 'CREATEPERSON', title:  '打印者', width: 155 }
				 ,{ field: 'PREPARE_SCHEMEID', title: '报告名称', width: 155 }
                ]],
                pagination: true,
                rownumbers: true

            });

             
        });
     

</script>

