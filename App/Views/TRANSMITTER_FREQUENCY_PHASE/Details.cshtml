<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/TRANSMITTER_FREQUENCY_PHASEApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#TEST_QUANTITY").html(res.TEST_QUANTITY);$("#GRADE").html(res.GRADE);$("#TEST_POINT").html(res.TEST_POINT);$("#TEST_POINT_UNIT").html(res.TEST_POINT_UNIT);$("#TWO_INPUT_STANDARD_VALUES").html(res.TWO_INPUT_STANDARD_VALUES);$("#TWO_INPUT_STANDARD_VALUES_UNIT").html(res.TWO_INPUT_STANDARD_VALUES_UNIT);$("#STANDARD_OUTPUT_VALUE").html(res.STANDARD_OUTPUT_VALUE);$("#STANDARD_OUTPUT_VALUE_UNIT").html(res.STANDARD_OUTPUT_VALUE_UNIT);$("#ACTUAL_OUTPUT_VALUE").html(res.ACTUAL_OUTPUT_VALUE);$("#ACTUAL_OUTPUT_VALUE_UNIT").html(res.ACTUAL_OUTPUT_VALUE_UNIT);$("#REFERENCE_ERROR").html(res.REFERENCE_ERROR);$("#REFERENCE_ERROR_UNIT").html(res.REFERENCE_ERROR_UNIT);$("#PREPARE_SCHEMEID").html(res.PREPARE_SCHEMEID);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);
            }
        })
    });
</script>

