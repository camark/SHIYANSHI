@model Langben.DAL.UNCERTAINTYPARAMETERMANAGEMENT

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.SOURCEOFERROR) :
                </div>
                <div class="display-field" id='SOURCEOFERROR'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.LIMIT_VALUE) :
                </div>
                <div class="display-field" id='LIMIT_VALUE'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.ERRORDISTRIBUTION) :
                </div>
                <div class="display-field" id='ERRORDISTRIBUTION'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.k) :
                </div>
                <div class="display-field" id='k'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UNCERTAINTYDEGREE) :
                </div>
                <div class="display-field" id='UNCERTAINTYDEGREE'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATETIME) :
                </div>
                <div class="display-field" id='UPDATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UPDATEPERSON) :
                </div>
                <div class="display-field" id='UPDATEPERSON'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATETIME) :
                </div>
                <div class="display-field" id='CREATETIME'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CREATEPERSON) :
                </div>
                <div class="display-field" id='CREATEPERSON'>
                </div>        
                <div class="display-label">
                      @Html.LabelFor(model => model.RULEID) :
                </div>
                <div class="display-field">
                    @if (Model.RULE != null)
                       {
                    @Model.RULE.NAME
                    } 
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/UNCERTAINTYPARAMETERMANAGEMENTApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#SOURCEOFERROR").html(res.SOURCEOFERROR);$("#LIMIT_VALUE").html(res.LIMIT_VALUE);$("#ERRORDISTRIBUTION").html(res.ERRORDISTRIBUTION);$("#k").html(res.k);$("#UNCERTAINTYDEGREE").html(res.UNCERTAINTYDEGREE);$("#UPDATETIME").html(res.UPDATETIME);$("#UPDATEPERSON").html(res.UPDATEPERSON);$("#CREATETIME").html(res.CREATETIME);$("#CREATEPERSON").html(res.CREATEPERSON);
            }
        })
    });
</script>

